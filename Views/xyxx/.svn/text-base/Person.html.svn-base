		<script type="text/javascript" language="javascript" class="init">
			$(document).ready(function() {
				var table = $('#data-table').DataTable( {
					//dom: 'Bftlpi',//组件初始化排序
					dom: 'tlpi',//组件初始化排序
					processing: true,//开启显示处理状态 
					autoWidth: false,
					scrollY:        800,//纵向滚动，限高
					scrollX:        true,//开启横向滚动
					scrollCollapse: true,//数据不够设定高度时，自动缩小
					paging: true,//开启分页（页码）
					pagingType: 'full_numbers',//页码显示模式，全部
					//pagingType: 'input',//页码显示模式，跳转页输入框
					lengthChange: true,//开启显示数据条数切换select
					lengthMenu: [ 10, 25, 50, 100],//数据条数选择
					
					fixedColumns:   {
						leftColumns: 3
					},//冻结列数量
					columnDefs: [ 
						{	
							orderable: false,//该列上禁止排序功能
							className: 'select-checkbox',
							targets:   0//目标列
						},
						{
							className: 'text-success',
							targets:   5//目标列
						},
						{
							orderable: false,//该列上禁止排序功能
							className: 'function-box',
							targets:   11//目标列
						}			
					],//列的初始属性
					select: {
						style:    'multi',
						selector: 'td'
					},//开启选择的方法
					order: [[ 1, 'asc' ]]//表格初始化排序
				} );

				table.select.info(false);//关闭选中提示消息

				var flag = 1;
				$('th.select-checkbox').on( 'click', function () {
					var $this = $(this),
						hasClass;

					if(flag){
						table.rows().select();
						$this.addClass("th-checked");
						flag = 0;
					}else{
						table.rows().deselect();
						$this.removeClass("th-checked");
						flag = 1;
					}
				} );
				$("#data-table_wrapper").append(
					"<div class='btn-group dataTables_goto'>"+ 
					"<a class='btn btn-default button-sm btn-outline disabled' href='javascript:;'>"+
					"<span>转到</span>"+"</a>"+
					"<input class='btn btn-default button-sm form-control page-enter' style=' type='text' id='PageIndexValue' name='PageIndexValue' />"+
					"<a class='btn btn-goto button-sm @gstr' href='#' onclick='goAjaxPgIndex(); return false;' title='转到指定页面'>GO</a>"+
					"</div>"
				);
			} );
		</script>
